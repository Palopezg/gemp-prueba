

entity Parametro {
  Id  Long required ,
  descripcion  String,
  valor String,
  businessId String
}

entity Producto {
  Id  Long required,
  descripcion  String,
  activo Int,
  
}



// Set pagination options
paginate Productos, Parametros with pagination

filter Parametros
filter Productos
 


